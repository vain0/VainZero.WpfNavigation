<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:demo="clr-namespace:VainZero.WpfNavigation.Demo"
    >
    <DataTemplate DataType="{x:Type demo:History}">
        <TreeView>
            <TreeViewItem
                IsExpanded="True"
                ItemsSource="{Binding BackEntries}"
                Header="Back"
                >
                <TreeViewItem.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding Title}" />
                    </DataTemplate>
                </TreeViewItem.ItemTemplate>
            </TreeViewItem>

            <TreeViewItem
                IsExpanded="True"
                ItemsSource="{Binding ForwardEntries}"
                Header="Forward"
                >
                <TreeViewItem.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding Title}" />
                    </DataTemplate>
                </TreeViewItem.ItemTemplate>
            </TreeViewItem>
        </TreeView>
    </DataTemplate>
</ResourceDictionary>
