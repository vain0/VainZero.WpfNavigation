<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:VainZero.WpfNavigationDemo.Views"
    xmlns:layoutFrames="clr-namespace:VainZero.WpfNavigationDemo.Views.LayoutFrames"
    xmlns:loginPages="clr-namespace:VainZero.WpfNavigationDemo.Views.LoginPages"
    xmlns:frontPages="clr-namespace:VainZero.WpfNavigationDemo.Views.FrontPages"
    xmlns:userEditing="clr-namespace:VainZero.WpfNavigationDemo.Views.Users.Editing"
    xmlns:userLists="clr-namespace:VainZero.WpfNavigationDemo.Views.Users.Lists"
    >
    <!-- Mapping from view models to views. -->

    <DataTemplate DataType="{x:Type loginPages:LoginPage}">
        <loginPages:LoginPageControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type frontPages:FrontPage}">
        <frontPages:FrontPageControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type userEditing:UserEditPage}">
        <userEditing:UserEditPageControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type userLists:UserListPage}">
        <userLists:UserListPageControl />
    </DataTemplate>
</ResourceDictionary>
